{
  "name": "@guanghechen/gatsby-transformer-yozora",
  "version": "1.2.1",
  "description": "Transform markdown files to Yozora AST",
  "author": {
    "name": "guanghechen",
    "url": "https://github.com/guanghechen/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/guanghechen/gatsby-scaffolds.git",
    "directory": "packages/gatsby-transformer-yozora"
  },
  "homepage": "https://github.com/guanghechen/gatsby-scaffolds/tree/master/packages/gatsby-transformer-yozora#readme",
  "keywords": [
    "gatsby",
    "gatsby plugin",
    "markdown",
    "gfm",
    "yozora markdown"
  ],
  "main": "gatsby-node.js",
  "types": "lib/types/index.d.ts",
  "source": "src/index.ts",
  "license": "MIT",
  "engines": {
    "node": ">= 14.15.0"
  },
  "files": [
    "lib/",
    "gatsby-node.js",
    "package.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "start": "cross-env NODE_ENV=development rollup -c -w ../../rollup.config.js",
    "build": "cross-env NODE_ENV=production rollup -c ../../rollup.config.js",
    "prebuild": "rimraf lib/",
    "prepublishOnly": "cross-env ROLLUP_SHOULD_SOURCEMAP=false yarn build"
  },
  "dependencies": {
    "@guanghechen/option-helper": "^1.4.1",
    "@yozora/ast": "^1.0.0-alpha.16",
    "@yozora/ast-util": "^1.0.0-alpha.16",
    "gray-matter": "^4.0.2"
  },
  "peerDependencies": {
    "gatsby": "^3.0.0-next.0"
  },
  "devDependencies": {
    "@yozora/core-parser": "^1.0.0-alpha.16"
